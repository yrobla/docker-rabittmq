exec erl -pa /usr/lib/rabbitmq/lib/rabbitmq_server-3.1.5/ebin -noinput -s rabbit boot -name rabbit@${CONTAINER_SERVER} -boot start_sasl +W w +K true +A30 +P 1048576 -kernel inet_dist_listen_max 9105 -kernel inet_dist_listen_min 9100 -kernel inet_default_connect_options '[{nodelay,true}]' -sasl errlog_type error -sasl sasl_error_logger false -rabbit error_logger '{file,"/var/lib/rabbitmq/error.log"}' -rabbit sasl_error_logger '{file,"/var/lib/rabbitmq/sasl.log"}' -rabbit enabled_plugins_file '"/etc/rabbitmq/enabled_plugins"' -rabbit plugins_dir '"/usr/lib/rabbitmq/lib/rabbitmq_server-3.1.5/plugins"' -rabbit plugins_expand_dir '"/var/lib/rabbitmq/mnesia/rabbit-plugins-expand"' -os_mon start_cpu_sup false -os_mon start_disksup false -os_mon start_memsup false -mnesia dir '"/var/lib/rabbitmq/mnesia/"' &
